# Tablenet with L = 16 centroids per group of Q = 1 activation.
# We quantize 1 x 5 x 5 input patches and we do 1 x 1 LUT.
# C_in = 1, C_out = 32.
# G = C_in / Q = 1.

# START Tablenet conv1
layer {
  name: "conv1_logit"
  type: "Convolution"
  bottom: "data"
  top: "conv1_logit"
  convolution_param {
    num_output: 16  # G * L
    group: 1  # G
    kernel_size: 5
    stride: 1
    weight_filler {
      type: "msra"
      variance_norm: FAN_IN
    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
  name: "conv1_logit_bn"
  type: "BatchNorm"
  bottom: "conv1_logit"
  top: "conv1_logit"
  param { lr_mult: 0 }
  param { lr_mult: 0 }
  param { lr_mult: 0 }
}

layer {
  name: "Anneal1"
  type: "Anneal"
  bottom: "conv1_logit"
  top: "conv1_logit"
  power_param {
	t_0: 0
	t_1: 5000
	scale_0: 1
	scale_1: 20
	batch_size: 64		
  }
}

layer {
  name: "conv1_softmax"
  type: "Softmax"
  bottom: "conv1_logit"
  top: "conv1_softmax"
  softmax_param {
      axis: 1
      outer_mul: 1  # G
  }
}
layer {
  name: "conv1_lut"
  type: "Convolution"
  bottom: "conv1_softmax"
  top: "conv1"
  convolution_param {
    num_output: 32  # C_out
    bias_term: false
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "msra"
      variance_norm: FAN_OUT
    }
  }
}
# END Tablenet conv1

layer {
  name: "conv1_bn"
  type: "BatchNorm"
  bottom: "conv1"
  top: "conv1"
  param { lr_mult: 0 }
  param { lr_mult: 0 }
  param { lr_mult: 0 }
}
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "conv1"
  top: "pool1"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}

