# Tablenet with L = 32 centroids per group of Q = 4 activations,
# We quantize 4 x 4 x 4 input patches and we do 1 x 1 LUT.
# C_in = 64, C_out = 512.
# G = C_in / Q = 16.

# START Tablenet ip1
layer {
  name: "ip1_logit"
  type: "Convolution"  # Convolution cause InnerProduct does not support group
  bottom: "pool2"
  top: "ip1_logit"
  convolution_param {
    num_output: 512  # G * L
    group: 16  # G
    kernel_size: 4
    stride: 1
    weight_filler {
      type: "msra"
      variance_norm: FAN_IN
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ip1_logit_bn"
  type: "BatchNorm"
  bottom: "ip1_logit"
  top: "ip1_logit"
  param { lr_mult: 0 }
  param { lr_mult: 0 }
  param { lr_mult: 0 }
}

layer {
  name: "Anneal3"
  type: "Anneal"
  bottom: "ip1_logit"
  top: "ip1_logit"
  power_param {
	t_0: 0
	t_1: 5000
	scale_0: 1
	scale_1: 20
	batch_size: 64		
  }
}

layer {
  name: "ip1_softmax"
  type: "Softmax"
  bottom: "ip1_logit"
  top: "ip1_softmax"
  softmax_param {
      axis: 1
      outer_mul: 16  # G
  }
}
layer {
  name: "ip1_lut"
  type: "Convolution"
  bottom: "ip1_softmax"
  top: "ip1"
  convolution_param {
    num_output: 512
    bias_term: false
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "msra"
      variance_norm: FAN_OUT
    }
  }
}
# END Tablenet ip1

layer {
  name: "ip1_bn"
  type: "BatchNorm"
  bottom: "ip1"
  top: "ip1"
  param { lr_mult: 0 }
  param { lr_mult: 0 }
  param { lr_mult: 0 }
}
layer {
  name: "relu1"
  type: "ReLU"
  bottom: "ip1"
  top: "ip1"
}
layer {
  name: "ip2"
  type: "InnerProduct"
  bottom: "ip1"
  top: "ip2"
  inner_product_param {
    num_output: 10
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "ip2"
  bottom: "label"
  top: "loss"
}
layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "ip2"
  bottom: "label"
  top: "accuracy"
  include {
    phase: TEST
  }
}
